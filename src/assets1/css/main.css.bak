.progress {
	width:175px;
	height:175px;
	border-radius: 50%;
	background: #FFF;
	box-shadow: 0px 5.489px 15.682px 1.568px rgba(0, 0, 0, 0.25);
	display:flex;
	justify-content:center;
	align-items:center;
	position: relative;
	margin-top: -113px;
	margin-bottom: 30px;
}
.progress-value > span{
	font-family: 'Gilroy';
	font-size:50px;
	font-style: normal;
	font-weight: 700;
	line-height: normal;
}
.progress-value > span strong{
	font-size: 24px;
    font-weight: 700;
}
.progress::after {
  content: "";
  width: 100%;
  height: 100%;
  border-radius: 50%;
  border: 15px solid #fff;
  position: absolute;
  top: 0;
  left: 0;
}
.progress>span {
  width: 50%;
  height: 100%;
  overflow: hidden;
  position: absolute;
  top: 0;
  z-index: 1;
}

.progress .progress-left {
  left: 0;
}

.progress .progress-bar {
  width: 100%;
  height: 100%;
  background: none;
  border-width: 15px;
  border-style: solid;
  position: absolute;
  top: 0;
}

.progress .progress-left .progress-bar {
  left: 100%;
  border-top-right-radius: 175px;
  border-bottom-right-radius: 175px;
  border-left: 0;
  -webkit-transform-origin: center left;
  transform-origin: center left;
}

.progress .progress-right {
  right: 0;
}

.progress .progress-right .progress-bar {
  left: -100%;
  border-top-left-radius: 175px;
  border-bottom-left-radius: 175px;
  border-right: 0;
  -webkit-transform-origin: center right;
  transform-origin: center right;
}
.progress .progress-value {
  position: absolute;
  top: 0;
  left: 0;
}
