/************* Start::Team Correlation Progress Area *********/
.team-correlation-area{
	margin-top:40px;
}
.team-correlation-wrapper{
	padding:0 80px;
}
.team-correlation-wrapper h2{
	color: #DA2439;
	font-family: 'Gilroy';
	font-size: 30px;
	font-style: normal;
	font-weight: 700;
	line-height: normal;
}
.correlation-group-heading{
	position:relative;
	width:7%;
	margin:20px 0;
}
.correlation-group-heading h4{
	color: #fff;
	font-family: 'Gilroy';
	font-size: 18px;
	font-style: normal;
	font-weight: 700;
	line-height: normal;
	position:absolute;
	top:50%;
	left:10%;
	transform:translatey(-50%);
	z-index:2;
}
.point-progress-bar-area{
	background: #F5F5F7;
	height:170px;
	display:flex;
	align-items:center;
	border-radius:40px;
	position:relative;
}
.point-progress-bar-area::before{
	content:'';
	position:absolute;
	width:5%;
	height:100%;
	top:50%;
	transform:translateY(-50%);
	left:0;
	background-color:#DA2439;
	border-radius:40px 0 0 40px;
}
.point-progress-bar-area::after{
	content:'';
	position:absolute;
	width:5%;
	height:100%;
	top:50%;
	transform:translateY(-50%);
	right:0;
	background-color:#2EBA0B;
	border-radius:0px 40px 40px 0;
}
.progress-barline{
	position:relative;
	background-color:#595858;
	width:90%;
	height:1px;
	margin:0 auto;
}
.each-side-progress-bar .progress-bar-amount{
	width:15px;
	height:15px;
	border-radius:50%;
	display:inline-block;
	position:absolute;
	top:50%;
	transform:translateY(-50%)
}
.negative-amount .progress-bar-amount{
	background-color:#DA2439;
}
.positive-amount .progress-bar-amount{
	background-color:#2EBA0B;
}
.each-side-progress-bar span{
	position:absolute;
	bottom:-37px;
	left:50%;
	transform:translatex(-50%);
	color: #000;
	font-family: 'Gilroy';
	font-size: 25px;
	font-style: normal;
	font-weight: 700;
	line-height: normal;
}
.progress-barline .each-side-progress-bar{
	position:relative;
	width:50%;
	display:flex;
}
.progress-barline .negative-amount::after{
	content:'-1';
	width:50px;
	height:50px;
	display:flex;
	align-items:center;
	justify-content:center;
	background-color:#DA2439;
	position:absolute;
	top:50%;
	transform:translatey(-50%);
	left:-25px;
	border-radius:50%;
	border:1px solid #fff;
	font-family: 'Gilroy';
	font-size: 25px;
	font-style: normal;
	font-weight: 700;
	line-height: normal;
}
.progress-barline .positive-amount::after{
	content:'-1';
	width:50px;
	height:50px;
	display:flex;
	align-items:center;
	justify-content:center;
	background-color:#2EBA0B;
	position:absolute;
	top:50%;
	transform:translatey(-50%);
	right:-25px;
	border-radius:50%;
	border:1px solid #fff;
	z-index:2;
	font-family: 'Gilroy';
	font-size: 25px;
	font-style: normal;
	font-weight: 700;
	line-height: normal;
}
.progress-barline .positive-amount{
	float:right;
}
.progress-barline .negative-progress-bar div{
	background-color:#DA2439;
}
.progress-bar-dividation {
    position: absolute;
    width: 1px;
    height: 35px;
    background-color: #000;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}
.progress-bar-dividation span{
	position: absolute;
    bottom: -30px;
    left: 50%;
    transform: translatex(-50%);
    color: #000;
    font-family: 'Gilroy';
    font-size: 25px;
    font-style: normal;
    font-weight: 700;
    line-height: normal;
}
.amount-point{
	position:absolute;
	top:-57px;
	text-align:center;
	color: #2EBA0B;
	font-family: 'Gilroy';
	font-size: 15px;
	font-style: normal;
	font-weight:700;
	line-height: normal;
	padding:2px 4px;
	border:1px solid #2EBA0B;
	transform:translatex(-50%);
	border-radius:5px;
}
.amount-point img{
	position:absolute;
	width:15%;
	bottom:-33px;
	left:50%;
	transform:translatex(-50%);
}
/************* End::Team Correlation Progress Area *********/
